{
    "name": "zzy-lsg/tencent-ad",
    "description": "Tencent Ad PHP SDK为了兼容新老版本",
    "type": "library",
    "require": {
        "php": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "ZzyLsg\\TencentAd\\": "src/TencentAds"
        }
    },
    "authors": [
        {
            "name": "linsonggao",
            "email": "371925293@qq.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2 || ^7.0",
        "monolog/monolog": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.cloud.tencent.com/composer/"
        },
        {
            "packagist": false
        }
    ],
    "config": {
        "secure-http": false
    },
    "scripts": {
        "phpcs": "vendor/bin/php-cs-fixer fix",
        "test": "vendor/bin/phpunit"
    }
}
